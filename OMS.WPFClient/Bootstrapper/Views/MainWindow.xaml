<Window x:Class="OMS.WPFClient.Bootsrapper.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:prismRegion="http://prismlibrary.com/"
        mc:Ignorable="d"
        prismRegion:ViewModelLocator.AutoWireViewModel="True"
        WindowStartupLocation="CenterScreen"
        WindowState="Maximized"
        TextElement.Foreground="{DynamicResource MaterialDesignBody}"
        TextElement.FontWeight="Medium"
        TextElement.FontSize="14"
        SnapsToDevicePixels="True"
        Title="Orders Management System">

    <ContentControl prismRegion:RegionManager.RegionName="GlobalRegion"/>
</Window>
